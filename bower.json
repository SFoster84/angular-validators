{
    "name": "angular-validators",
    "version": "0.1.0",
    "main": [
        "./src/*.js"
        "!./src/*.js"
    ],
    "ignore": [
        "config",
        "node_modules",
        "src",
        "test",
        ".travis.yml",
        ".gitignore",
        ".jshintrc",
        "CHANGELOG.md",
        "Gruntfile.js",
        "package.json"
    ],
    "dependencies": {
        "angular": "1.3.x"
    },
    "devDependencies": {
        "angular-mocks": "1.3.x",
        "jquery": "latest"
    }
}
